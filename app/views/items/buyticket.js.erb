$('.numtick').fadeOut(function(){
	var numticks = $(this).html();
	var newnum = parseInt(numticks)+1;
	$(this).html(newnum).fadeIn();
});

$('.coolcredits').fadeOut(function(){
	var numticks = $('.price').html();
	var numticks2 = $(this).html();
	var newnum = parseInt(numticks2) - parseInt(numticks);
	$(this).html(newnum).fadeIn();
});
